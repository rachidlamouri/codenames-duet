{
  "scripts": {
    "build": "webpack",
    "lint": "eslint --ext js,vue .",
    "start": "node src/server",
    "test:functional": "cucumber-js tests/functional/features --require \"tests/functional/support/*.js\"",
    "test:unit": "jest"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run build && npm run test:unit"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^components/(.*)$": "<rootDir>/src/components/$1",
      "^helpers/(.*)$": "<rootDir>/tests/helpers/$1",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^store/(.*)$": "<rootDir>/src/store/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/bootstrapVue.js",
      "<rootDir>/tests/setup.js"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      ".*\\.(js)$": "babel-jest"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@cucumber/cucumber": "^7.0.0",
    "@randograms/schema-to-generator": "^0.5.0",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "bluebird": "^3.7.2",
    "chai": "^4.3.0",
    "chai-json-schema-ajv": "^5.2.3",
    "css-loader": "3.2.0",
    "eslint": "7.15.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-vue": "7.2.0",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.5",
    "jest": "^26.6.3",
    "jest-json-schema": "^2.1.0",
    "jest-serializer-vue": "^2.0.2",
    "puppeteer": "^7.1.0",
    "sass": "^1.32.0",
    "sass-loader": "10.1.0",
    "vue": "2.6.12",
    "vue-jest": "^3.0.7",
    "vue-loader": "15.9.5",
    "vue-template-compiler": "2.6.12",
    "vue-template-loader": "1.1.0",
    "vuex": "^3.6.0",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
